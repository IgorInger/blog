<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://igor-inger.de/jsf/html</namespace>
	<tag>
		<tag-name>inputFile</tag-name>
		<component>
			<component-type>HtmlInputFile</component-type>
		</component>		
	</tag>
	<tag>
		<tag-name>latex</tag-name>
		<component>
			<component-type>HtmlOutputLatex</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>convertComposite</tag-name>
		<converter>
			<converter-id>composite</converter-id>
			<handler-class>de.inger.blog.faces.converter.CompositeConverterHandler</handler-class>
		</converter>
		<attribute>
			<name>converters</name>
			<type>java.jang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>paginator</tag-name>
		<component>
			<component-type>HtmlOutputPaginator</component-type>			
		</component>
		<attribute>
			<name>pages</name>
			<required>true</required>
			<type>java.lang.Integer</type>
		</attribute>
		<attribute>
			<name>page</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
	</tag>
</facelet-taglib>